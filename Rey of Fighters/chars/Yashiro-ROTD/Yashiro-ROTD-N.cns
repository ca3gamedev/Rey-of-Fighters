[Statedef 100]
type    = S
physics = S
anim = 100
ctrl = 0
sprpriority = 1

[State 100, VelSet]
type = VelSet
trigger1 = 1
x = const(velocity.run.fwd.x)

[State 100, AssertSpecial]
type = AssertSpecial
trigger1 = 1
flag = NoWalk

[State 100, AssertSpecial]
type = AssertSpecial
trigger1 = 1
flag = NoAutoTurn

[State 100, PlaySnd]
type = PlaySnd
trigger1 = StateNo = 100 && (AnimElem = 2 || AnimElem = 6)
value = 1,0
channel = 4

[State 100, ChangeState]
type = ChangeState
trigger1 = command = "holdfwd" && command = "holdup"
value = 40

[State 100, ChangeState]
type = ChangeState
trigger1 = command != "holdfwd" 
value = 101

