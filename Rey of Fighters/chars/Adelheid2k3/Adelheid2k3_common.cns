trigger1 = Time = 0
trigger1 = P2Dist X < -20
value = 45

[State 40, 1]
type = VarSet
trigger1 = Time = 0
sysvar(1) = 0

[State 40, 2]
type = VarSet
trigger1 = Time = 0
sysvar(1) = (command = "holdfwd") - (command = "holdback")

[State 40, v3];大ジャンプ
type = VarSet
trigger1 = Time = 0
var(3) = 0

[State 40, v3]
type = VarSet
triggerall = Time = 0
triggerall = sysvar(1)
trigger1 = PrevStateNo = 100 || PrevStateNo = 101
trigger2 = Command = "longjump"
trigger3 = Command = "longjump" && Command = "holdfwd"
trigger4 = Command = "longjump" && Command = "holdback"
