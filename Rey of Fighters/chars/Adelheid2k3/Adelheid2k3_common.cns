[State 40, v4];小ジャンプ
type = VarSet
trigger1 = Time = 0
var(4) = 0

[State 40, v4]
type = VarSet
trigger1 = command != "holdup"
var(4) = 1

[State 40, vel x]
type = VelSet
trigger1 = AnimTime = 0
trigger1 = !var(3)
x = ifelse(sysvar(1)=0, 0, ifelse(sysvar(1)=1, const(velocity.jump.fwd.x), const(velocity.jump.back.x)))

[State 40, vel x]
type = VelSet
trigger1 = AnimTime = 0
trigger1 = var(3)
x = ifelse(sysvar(1)=0, 0, ifelse(sysvar(1)=1, const(velocity.runjump.fwd.x), const(velocity.runjump.back.x)))

[State 40, 8]
type = VelSet
trigger1 = AnimTime = 0
y = const(velocity.jump.y)*IfElse(var(4),0.75,1)

[State 40, end]
type = ChangeState
trigger1 = AnimTime = 0
value = 50
ctrl = 1


