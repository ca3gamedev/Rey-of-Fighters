;---------------------------------------------------------------------------
; 基本行動のステート
;---------------------------------------------------------------------------
; 挑発
; CNS レベル: 基本
[Statedef 160]
type     = S
movetype = I
physics  = S
anim     = 160
velset   = 0,0
ctrl     = 0
sprpriority = 2 ;ここで表示優先度を決定できます。　範囲は-5〜5、大きいほど手前に表示される
facep2 = 1      ;相手の方を向いてから挑発（結構重要）

;------------------------
; いっしょにトレーニング
;------------------------
[State 160, VarSet]
type = VarSet
triggerall = var(40) > 0
trigger1 = var(39) = 1
trigger1 = time = 0
var(40) = 0

[State 160, ガード弾き値・リセット]
type = VarSet
triggerall = fvar(10) != 0
trigger1 = var(39) = 1
trigger1 = Time = 0
fvar(10) = 0

[State 160, 気絶値・リセット]
type = VarSet
triggerall = fvar(13) != 0
trigger1 = var(39) = 1
trigger1 = Time = 0
fvar(13) = 0

;------------------------

[State 160, CtrlSet]
type = CtrlSet
triggerall = Time >= 33
triggerall = !winko
trigger1 = var(59) != 1
trigger2 = var(59) = 1
trigger2 =!(EnemyNear(var(58)),stateno = 5715 && EnemyNear(var(58)),anim = 5300 && EnemyNear(var(58)),movetype = I)
trigger2 = InGuardDist || EnemyNear(var(58)),MoveType = A || EnemyNear(var(58)),Vel X > 0
value = 1

[State 160, PlaySnd]
type = PlaySnd
trigger1 = AnimElem = 1
value = s10,0
channel = 0

[State 160, ChangeState]
type = ChangeState
trigger1 = AnimTime = 0
value = 0
ctrl = 1

