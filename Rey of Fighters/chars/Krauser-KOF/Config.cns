;-----------------------------------------------------------------------------
;設定項目
;-----------------------------------------------------------------------------
[Statedef 40000]
anim = 9999
ctrl = 0

;-----------------------------------------------------------------------------
;本体システム設定
;-----------------------------------------------------------------------------
;MUGENバージョン設定(MUGEN Version Setting)
; var(0) = 0･･･MUGEN 1.0
; var(0) = 1･･･WinMugen
[State 40000, Setting]
Type = parentvarset
Trigger1 = 1
var(0) = 1




;-----------------------------------------------------------------------------
;AI設定
;-----------------------------------------------------------------------------
;AIレベル設定
;　var(57)の数値を1〜8の間で変更して下さい。
;　新Mugenの場合、AILevelでも代用可能です。
;　　1〜2････Easy
;　　3〜4････Normal
;　　5〜6････Hard
;　　7〜8････対AI特化
[State 40000, Setting]
Type = parentvarset
trigger1 = !Parent,var(57)
var(57) = 8

;反応速度設定
;　var(56)の数値を1〜10の間で変更して下さい。
;　反応速度50F(レベル0)からレベルを1上げるごとに5Fずつ速くなります。10で反応制限なし。
[State 40000, Setting]
Type = parentvarset
trigger1 = !Parent,var(56)
var(56) = 10

;挑発設定
;　var(55)の数値を1(以上)にすると、レベルと反応速度を上げるために挑発を使用してきます。
[State 40000, Setting]
Type = parentvarset
trigger1 = 1
var(55) = 0

;-----------------------------------------------------------------------------
;設定項目はここまで。以下はいじらないでください
;-----------------------------------------------------------------------------

[State 6000]
type = AssertSpecial
Trigger1 = 1
flag = NoShadow
flag2 = invisible

[State 6000]
type = NotHitBy
Trigger1 = 1
value = SCA,AA,AT,AP

[State 6000]
type = DestroySelf
trigger1 = RoundState > 1
