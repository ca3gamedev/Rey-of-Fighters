[Data]
life = 1000 		;You can easily get accurate value for NeoGeo KOF's using Artmoney, and XI's are around the internet.
attack = 100		;Leave it alone for compatibility.
defence = 100		;Leave it alone for compatibility.
fall.defence_up = 90	;0 for accuracy.
liedown.time = 20		;It's aways 9 in KOF games. SVC uses 12, probably for Demitri's Pursuit Attack to work, but I suggest to leave it alone.
airjuggle = 15		;We'll not use it, but aways keep it for compatibility.
sparkno = 2		;Leave it alone for compatibility.
guard.sparkno = 40	;Leave it alone for compatibility.
KO.echo = 0		;KOF does not use it, NGBC does.
volume = 255		;Manage your global sound volume here.
power = 5000

;Variables with this index and above will not have their values
;reset to 0 between rounds or matches. There are 60 int variables,
;indexed from 0 to 59, and 40 float variables, indexed from 0 to 39.
;If omitted, then it defaults to 60 and 40 for integer and float
;variables repectively, meaning that none are persistent, i.e. all
;are reset. If you want your variables to persist between matches,
;you need to override state 5900 from common1.cns.
IntPersistIndex = 58	;
FloatPersistIndex = 30

[Size]
xscale = 1	          ;Horizontal scaling factor.
yscale = 1	          ;Vertical scaling factor.
ground.back = 16 	          ;Player width (back, ground)
ground.front = 16	          ;Player width (front, ground)
air.back = 12   	          ;Player width (back, air)
air.front = 12    	          ;Player width (front, air)
height = 60      	          ;Height of player (for opponent to jump over)
attack.dist = 160  	          ;Default attack distance
proj.attack.dist = 90         ;Default attack distance for projectiles
proj.doscale = 0              ;Set to 1 to scale projectiles too
head.pos = -16, -95         ;Approximate position of head
mid.pos = -5, -60            ;Approximate position of midsection
shadowoffset = 0            ;Number of pixels to vertically offset the shadow
draw.offset = 0,0            ;Player drawing offset in pixels (x, y)

[Velocity]
walk.fwd  = 3.1 	  		          ;Walk forward velocity.
walk.back = -3.2 	  		 ;Walk backward velocity, usually is the same as walk.fwd, but not aways.
run.fwd  = 6.3 	 		         ;Run forward (x, y) velocity.
run.back = -10.9750518798828125,-0.78515625	 ;Hop backward (x, y) velocity.
jump.neu = 0,-9.13671875			 ;Neutral jumping velocity (x, y) velocity. X vel is aways 0.
jump.back = -3.30078125   			;Jump back Speed (x, y) velocity.
jump.fwd = 3.30078125     		         ;Jump forward Speed (x, y) velocity.
runjump.back = -4.951171875, -9.13671875	 ;Running jump back velocity. Y velocity is aways the same as jump.neu.
runjump.fwd = 4.951171875, -9.13671875    	 ;Running jump forward velocity. Y velocity is aways the same as jump.neu.
airjump.neu = 0,-8.1      			 ;No need to mess with it, has no use fo KOF characters.
airjump.back = -2.55    		 	 ;No need to mess with it, has no use fo KOF characters.
airjump.fwd = 2.5        		 	 ;No need to mess with it, has no use fo KOF characters.

[Movement]
