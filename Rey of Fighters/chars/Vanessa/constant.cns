airjump.height = 0  			;Minimum distance from ground before you can air jump, no need to worry about it.
yaccel = 0.49920000      			;Vertical acceleration.
stand.friction = .82  			;Friction coefficient when standing. For KOF accuracy, aways use this value.
crouch.friction = .82 			;Friction coefficient when standing. For KOF accuracy, aways use this value.

