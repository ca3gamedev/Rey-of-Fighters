Ctrl = 1

[State -1, AI]
Type = ChangeState
triggerall = (stateno=19||stateno=21)&&(var(59)>0)
trigger1 = time > 12 || var(59)>8
trigger1 = P2bodydist x < 30
Trigger1 = anim = 20
trigger2 = time > 24 || var(59)>8
trigger2 = P2bodydist x >= 90
Trigger2 = anim = 21
trigger3 = inguarddist
