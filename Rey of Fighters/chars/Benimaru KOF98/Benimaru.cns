;---------------------------------------------------------------------------
; 基本行動のステート
;---------------------------------------------------------------------------
; 挑発
; CNS レベル: 基本
[Statedef 160]
type     = S
movetype = I
physics  = N
anim     = 160
velset   = 0,0
ctrl     = 0
sprpriority = 2 ;ここで表示優先度を決定できます。　範囲は-5～5、大きいほど手前に表示される
facep2 = 1      ;相手の方を向いてから挑発（結構重要）

;------------------------
; いっしょにトレーニング
;------------------------
[State 160, VarSet]
type = VarSet
triggerall = var(40) > 0
trigger1 = var(39) = 1
trigger1 = time = 0
var(40) = 0

[State 160, ガード弾き値・リセット]
type = VarSet
triggerall = fvar(10) != 0
trigger1 = var(39) = 1
trigger1 = Time = 0
fvar(10) = 0

[State 160, 気絶値・リセット]
type = VarSet
triggerall = fvar(13) != 0
trigger1 = var(39) = 1
trigger1 = Time = 0
fvar(13) = 0

;------------------------

[State 160, コントロール可否]
type = CtrlSet
triggerall = Time >= 33
triggerall = !winko
trigger1 = var(59) != 1
trigger2 = var(59) = 1
trigger2 =!(EnemyNear(var(58)),stateno = 5715 && EnemyNear(var(58)),anim = 5300 && EnemyNear(var(58)),movetype = I)
trigger2 = InGuardDist || EnemyNear(var(58)),MoveType = A || EnemyNear(var(58)),Vel X > 0
value = 1

[State 160, PlaySnd]
type = PlaySnd
trigger1 = AnimElem = 1
value = s10,30
channel = 0

[State 160, PosAdd]
type = PosAdd
trigger1 = animelem = 2
trigger2 = animelem = 3
trigger3 = animelem = 4
x = -8

[State 160, PosAdd]
type = PosAdd
trigger1 = animelem = 10
trigger2 = animelem = 11
trigger3 = animelem = 12
x = 8

[State 160, ステート変更]
type = ChangeState
trigger1 = AnimTime = 0
value = 0
ctrl = 1

;---------------------------------------------------------------------------
; 勝利ポーズ分岐
; CNS レベル: 基本
;
;ここで勝利ポーズを選択する。　MatchOverは試合終了を判定するので、
;一回目と二回目勝利を別のポーズにしたい時に便利。

[Statedef 180]
type = S
ctrl = 0
;anim = 180
velset = 0,0

[State 180, VarSet]
type = VarSet
trigger1 = command = "holdx"
trigger2 = command != "holdx" && command != "holda" && command != "holdy" && command != "holdb"
trigger2 = Random%4 = 0
var(0) = 0

[State 180, VarSet]
type = VarSet
trigger1 = command = "holda"
trigger2 = command != "holdx" && command != "holda" && command != "holdy" && command != "holdb"
trigger2 = Random%4 = 1
var(0) = 1

[State 180, VarSet]
type = VarSet
trigger1 = command = "holdy"
trigger2 = command != "holdx" && command != "holda" && command != "holdy" && command != "holdb"
trigger2 = Random%4 = 2
var(0) = 2

[State 180, VarSet]
type = VarSet
trigger1 = command = "holdb"
trigger2 = command != "holdx" && command != "holda" && command != "holdy" && command != "holdb"
trigger2 = Random%4 = 3
var(0) = 3

[State 180, NotHitBy]
type = NotHitBy
trigger1 = 1
value = SCA

[State 180, AssertSpecial]
type = AssertSpecial
trigger1 = 1
flag = roundnotover

[State 180, ChangeAnim]
type = ChangeAnim
trigger1 = Anim != 0
value = 0

[State 180, Explod]
type = Explod
trigger1 = wintime = 0
trigger1 = numhelper(8100) = 0
trigger1 = teamside = 1
anim = 8110
ownpal = 1
pos = 8,45
postype = left
bindtime = -1
ontop = 1
persistent = 0

[State 180, Explod]
type = Explod
trigger1 = wintime = 0
trigger1 = numhelper(8100) = 0
trigger1 = teamside = 2
anim = 8111
ownpal = 1
pos = 8,45
postype = left
bindtime = -1
ontop = 1
persistent = 0

[State 180, VarSet]
type = VarSet
trigger1 = time = 0
var(16) = 0

[State 180, VarAdd]
type = VarAdd
trigger1 = wintime = 0
trigger1 = numhelper(8100) = 0
var(16) = 1

[State 180, PlaySnd]
type = PlaySnd
trigger1 = wintime = 0
trigger1 = var(16) > 16
trigger1 = numhelper(8100) = 0
